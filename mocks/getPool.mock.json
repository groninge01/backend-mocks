{
  "name": "getPool",
  "request": {
    "url": "/graphql",
    "method": "POST",
    "headers": {
      "Content-Type": "application/json"
    },
    "body": {
      "operationName": "GetPool"
    }
  },
  "responses": {
    "ok": {
      "default": true,
      "status": 200,
      "file": "data/getPool.json"
    },
    "with userbalance": {
      "status": 200,
      "file": "data/getPool_with_user_balance.json"
    },
    "with hook": {
      "status": 200,
      "file": "data/getPool_with_hook.json"
    },
    "with nested hook": {
      "status": 200,
      "file": "data/getPool_with_nested_hook.json"
    },
    "with unreviewed hook": {
      "status": 200,
      "file": "data/getPool_with_unreviewed_hook.json"
    },
    "with hooks": {
      "status": 200,
      "file": "data/getPool_with_hooks.json"
    },
    "with dynamic swap fee": {
      "status": 200,
      "file": "data/getPool_with_dynamic_swap_fee.json"
    },
    "with lbp before": {
      "status": 200,
      "file": "data/getPool_with_lbp_before.json"
    },
    "with lbp running": {
      "status": 200,
      "file": "data/getPool_with_lbp_running.json"
    },
    "with lbp after": {
      "status": 200,
      "file": "data/getPool_with_lbp_after.json"
    }
  }
}
